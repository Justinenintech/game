params.put(&quot;orderId&quot;,UUID.randomUUID().toString().replace(&quot;-&quot;, &quot;&quot;));
params.put(&quot;type&quot;, 2);
params.put(&quot;money&quot;, money);

vars.put(&quot;param&quot;,params.toJSONString());



String timestamp =&quot;${__time(,)}&quot;;

String num = ThreadLocalRandom.current().nextInt(10000000, 99999999) + &quot;&quot;;

String sign = DigestUtils.md5Hex(rootKey + agentCode + secretKey + params.toJSONString() + timestamp + num);
//System.out.println(sign);
vars.put(&quot;num&quot;,num);
vars.put(&quot;sign&quot;,sign);
vars.put(&quot;timestamp&quot;, timestamp);






	







